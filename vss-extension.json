{
    "manifestVersion": 1,
    "id": "KatalonExtensionTask",
    "name": "Katalon Extension Plugin",
    "version": "0.0.1",
    "publisher": "AnhTuan",
    "public": false,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "This Plugin is for execute Katalon Project in Azure Devops",
    "categories": [
        "Azure Pipelines"
    ],
    "tags": [
        "Katalon",
        "Katalon Studio"
    ],
    "icons": {
        "default": "images/extension-icon.png"        
    },
    "files": [
        {
            "path": "katalonTask"
        }
        
    ],
    "content": {
        "details": {
          "path": "overview.md"
        }
      },
    "link": {
        "repository": {
            "uri": "https://forum.katalon.com/"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/katalon-studio"
      },
    "contributions": [
        {
            "id": "KatalonExtensionTask",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "katalonTask"
            }
        }
    ]
}